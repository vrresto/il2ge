%pragma(java) modulecode=%{

  public static final boolean IS_LOADED = isLoaded();

  private static boolean isLoaded()
  {
    try
    {
      if (getInterfaceVersion() != ${interface_version})
      {
        throw new Exception("Interface version mismatch.");
      }
    }
    catch (Throwable e)
    {
      System.err.println("IL2GE interface initialisation failed:\n" + e);
      return false;
    }

    return true;
  }

%}
